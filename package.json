{
  "name": "colivara-ts",
  "version": "1.0.0",
  "description": "Colivara TypeScript SDK for API integration",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "test": "NODE_NO_WARNINGS=1 jest --coverage",
    "test:watch": "jest --watch",
    "test:specific": "jest -t",
    "clean": "rm -rf dist",
    "start": "ts-node draft.ts",
    "prebuild": "npm run clean",
    "lint": "eslint . --ext .ts",
    "prepack": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tjmlabs/colivara-ts.git"
  },
  "keywords": [
    "colivara",
    "sdk",
    "api",
    "typescript",
    "tjmlabs"
  ],
  "author": {
    "name": "TJM labs",
    "email": "founders@colivara.com"
  },
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/tjmlabs/colivara-ts/issues"
  },
  "homepage": "https://github.com/tjmlabs/colivara-ts#readme",
  "dependencies": {
    "axios": "^1.7.9",
    "svix": "^1.44.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/mime": "^3.0.4",
    "@types/mime-types": "^2.1.4",
    "@types/node": "^22.10.2",
    "axios-mock-adapter": "^2.1.0",
    "jest": "^29.7.0",
    "mime-types": "^2.1.35",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.2"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testMatch": [
      "**/__tests__/**/*.test.ts"
    ],
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "text",
      "lcov",
      "html"
    ],
    "collectCoverageFrom": [
      "client.ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>"
    ],
    "roots": [
      "<rootDir>"
    ],
    "modulePaths": [
      "<rootDir>"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
